{
  "targetDefaults": {
    "build": {
      "cache": true
    },
    "lint": {
      "cache": true,
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"]
    },
    "test": {
      "cache": true,
      "inputs": ["default", "^default"]
    },
    "e2e": {
      "cache": true
    }
  },
  "extends": "@nx/workspace/presets/core.json",
  "affected": {
    "defaultBase": "main"
  },
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": false
    }
  }
}
